{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "ESNext",
		"target": "es2017",
		"importsNotUsedAsValues": "error",
		"checkJs": true,
		"isolatedModules": true,
		"sourceMap": true,
		"types": [
			"svelte",
			"node",
			"webdriverio/async",
			"@wdio/mocha-framework"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "."
	}
}
