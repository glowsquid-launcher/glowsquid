{
	"name": "@glowsquid/glow-ui",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"dev": "run-p 'dev:*'",
		"dev:storybook": "yarn run storybook",
		"dev:package": "watch 'yarn run build:package' ./src",
		"dev:test": "vitest --dir ./src",
		"build": "run-p 'build:*'",
		"build:sync": "svelte-kit sync",
		"build:package": "svelte-kit package",
		"prepare": "svelte-kit sync",
		"format": "eslint --fix --ext .ts,.svelte",
		"test": "vitest run --dir ./src",
		"coverage": "vitest run --coverage",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"devDependencies": {
		"@babel/core": "^7.18.5",
		"@glowsquid/eslint-config": "workspace:*",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@rushstack/eslint-patch": "^1.1.3",
		"@storybook/addon-a11y": "^6.5.9",
		"@storybook/addon-actions": "^6.5.9",
		"@storybook/addon-docs": "^6.5.9",
		"@storybook/addon-essentials": "^6.5.9",
		"@storybook/addon-interactions": "^6.5.9",
		"@storybook/addon-links": "^6.5.9",
		"@storybook/addon-storysource": "^6.5.9",
		"@storybook/addon-svelte-csf": "^2.0.4",
		"@storybook/builder-vite": "^0.1.37",
		"@storybook/svelte": "^6.5.9",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@testing-library/svelte": "^3.1.3",
		"babel-loader": "^8.2.5",
		"chromatic": "^6.6.3",
		"eslint": "^8.18.0",
		"jsdom": "^20.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"storybook-dark-mode": "^1.1.0",
		"svelte": "^3.48.0",
		"svelte-check": "^2.8.0",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.11",
		"tslib": "^2.4.0",
		"typescript": "~4.7.4",
		"vitest": "^0.17.1",
		"vitest-svelte-kit": "^0.0.6",
		"watch": "^1.0.2"
	},
	"type": "module",
	"$schema": "http://json.schemastore.org/package.json",
	"module": "./package/index.js",
	"dependencies": {
		"@svelteuidev/composables": "^0.6.9",
		"@svelteuidev/core": "^0.6.9",
		"@svelteuidev/motion": "^0.6.9"
	}
}
