{
  "name": "@glowsquid/glow-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p dev:storybook dev:package",
    "dev:storybook": "yarn run storybook",
    "dev:package": "watch 'yarn run package' ./src",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.5.0-beta.0",
    "@storybook/addon-docs": "^6.5.0-beta.0",
    "@storybook/addon-essentials": "^6.5.0-beta.0",
    "@storybook/addon-interactions": "^6.5.0-beta.0",
    "@storybook/addon-links": "^6.5.0-beta.0",
    "@storybook/addon-svelte-csf": "^2.0.3",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/svelte": "^6.5.0-beta.0",
    "@storybook/testing-library": "^0.0.11",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-loader": "^8.2.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-svelte3": "^3.2.1",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "vitest": "^0.9.4",
    "watch": "^1.0.2"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package/package.json",
    "./components/button/Button.spec": "./package/components/button/Button.spec.js",
    "./components/button/Button.stories.svelte": "./package/components/button/Button.stories.svelte",
    "./components/button/Button.svelte": "./package/components/button/Button.svelte",
    ".": "./package/index.js"
  },
  "svelte": "./package/index.js"
}
