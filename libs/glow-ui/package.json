{
  "name": "@glowsquid/glow-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:storybook": "yarn run storybook",
    "dev:package": "watch 'yarn run build:package' ./src",
    "dev:test": "vitest",
    "build": "run-p 'build:*'",
    "build:build": "svelte-kit build",
    "build:package": "svelte-kit package",
    "prepare": "svelte-kit sync",
    "format": "eslint --fix ./src --ext .ts,.svelte",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addon-svelte-csf": "^2.0.3",
    "@storybook/builder-vite": "^0.1.30",
    "@storybook/svelte": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-loader": "^8.2.5",
    "eslint": "^8.0.1",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "eslint-plugin-vitest": "^0.0.8",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.9",
    "tslib": "^2.4.0",
    "typescript": "~4.6.4",
    "vitest": "^0.10.0",
    "vitest-svelte-kit": "^0.0.6",
    "watch": "^1.0.2"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package/package.json",
    "./components/button/Button.svelte": "./package/components/button/Button.svelte",
    "./helpers": "./package/helpers.js",
    "./themes.css": "./package/themes.ccs",
    ".": "./package/index.js"
  },
  "svelte": "./package/index.js"
}
