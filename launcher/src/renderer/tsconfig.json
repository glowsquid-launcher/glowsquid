{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@types/node",
      "@nuxt/vue-app",
      "@nuxtjs/vuetify",
      "@nuxtjs/axios",
      "nuxt-i18n",
      "@nuxtjs/toast"
    ]
  }
}
